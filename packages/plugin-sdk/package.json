{
  "name": "@naap/plugin-sdk",
  "version": "0.1.0",
  "description": "SDK for developing NAAP plugins",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "naap-plugin": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./integrations": {
      "import": "./dist/integrations/index.js",
      "types": "./dist/integrations/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./testing": {
      "import": "./dist/testing/index.js",
      "types": "./dist/testing/index.d.ts"
    },
    "./umd": {
      "import": "./dist/umd/index.js",
      "require": "./dist/umd/naap-plugin-sdk.js",
      "types": "./dist/umd/naap-plugin-sdk.d.ts"
    },
    "./ai": {
      "import": "./dist/ai/index.js",
      "types": "./dist/ai/index.d.ts"
    },
    "./config": {
      "import": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./contracts": {
      "import": "./dist/contracts/index.js",
      "types": "./dist/contracts/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:umd": "node scripts/build-umd.js",
    "build:all": "npm run build && npm run build:umd",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:umd": "vitest run --config vitest.umd.config.ts"
  },
  "dependencies": {
    "@naap/types": "*",
    "ajv": "^8.12.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "execa": "^8.0.0",
    "fs-extra": "^11.2.0",
    "graphql": "^16.12.0",
    "inquirer": "^9.2.0",
    "ora": "^8.0.0",
    "semver": "^7.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/react": "^14.2.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.0.0",
    "@types/semver": "^7.5.8",
    "@vitest/coverage-v8": "^4.0.18",
    "jsdom": "^24.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "typescript": "~5.8.2",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "keywords": [
    "naap",
    "plugin",
    "sdk",
    "micro-frontend"
  ],
  "author": "NAAP Team",
  "license": "MIT"
}
