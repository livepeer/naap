{
  "$schema": "https://plugins.naap.io/schema/plugin.json",
  "name": "orchestrator-manager",
  "displayName": "Orchestrator Manager",
  "version": "1.0.0",
  "description": "Manage GPU compute orchestrators on the network with real-time monitoring, performance metrics, and fleet management.",
  "author": {
    "name": "NAAP Team",
    "email": "team@naap.io",
    "url": "https://naap.io"
  },
  "repository": "https://github.com/naap/plugins/tree/main/orchestrator-manager",
  "license": "MIT",
  "keywords": ["orchestrator", "gpu", "compute", "monitoring", "ai"],
  "category": "infrastructure",
  
  "shell": {
    "minVersion": "0.1.0",
    "maxVersion": "2.x"
  },
  
  "frontend": {
    "entry": "./frontend/dist/production/orchestrator-manager.js",
    "devPort": 3002,
    "routes": [
      "/orchestrators",
      "/orchestrators/*"
    ],
    "navigation": {
      "label": "Orchestrators",
      "icon": "Cpu",
      "order": 20,
      "group": "infrastructure"
    }
  },
  
  "backend": {
    "entry": "./backend/dist/server.js",
    "devPort": 4002,
    "port": 4102,
    "healthCheck": "/healthz",
    "apiPrefix": "/api/v1/orchestrator-manager",
    "resources": {
      "memory": "256Mi",
      "cpu": "0.25"
    }
  },
  
  "integrations": {
    "required": [],
    "optional": []
  },
  
  "permissions": {
    "shell": ["navigation", "notifications", "theme"],
    "apis": [],
    "external": []
  },
  
  "config": {
    "schema": {
      "refreshInterval": {
        "type": "number",
        "default": 30,
        "description": "Metrics refresh interval in seconds"
      }
    }
  }
}
